{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "noEmit": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "lib": ["ES5", "DOM", "ES2015.Promise", "ES2015.Collection"],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "*"
  ]
}
